Algoritmo _05_calculHipotenusa
	Definir catet1 como real;
	Definir catet2 como real;
	Escribir "Indica el catet a: ";
	Leer catet1;
	Escribir "Indica el catet b: ";
	Leer catet2;
	Definir hipotenusa como real;
	hipotenusa <- (catet1*catet1)+(catet2*catet2);
	Definir resultat como real;
	resultat <- rc(hipotenusa);
	Escribir "La hipotenusa es: ",resultat;
	
FinAlgoritmo
	